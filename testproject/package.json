{
  "name": "tmap-plus",
  "version": "4.1.0",
  "author": "",
  "private": true,
  "scripts": {
    "lint": "eslint ./src",
    "lint-fix": "eslint --fix ./src",
    "server": "node ./jsonServer/server.js",
    "start": "react-scripts start",
    "dev": "concurrently \"yarn run server\" \"yarn start\"",
    "build": "react-scripts build",
    "build:dev": "env-cmd -f .env env-cmd -f .env.dev env-cmd -f .env.dev.local --silent react-scripts build",
    "build:dtg": "env-cmd -f .env env-cmd -f .env.dtg env-cmd -f .env.dtg.local --silent react-scripts build",
    "build:stg": "env-cmd -f .env env-cmd -f .env.stg env-cmd -f .env.stg.local --silent react-scripts build",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": [
      "react-app"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
